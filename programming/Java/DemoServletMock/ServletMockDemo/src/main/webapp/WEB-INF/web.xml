<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>testedServlet</servlet-name>
		<servlet-class>servlet.mock.demo.TestedServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
